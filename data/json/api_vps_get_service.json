{
    "name": "api_vps_get_service",
    "desc": "This Function Applies to the VPS services.\nGets service info for the given ID in the given Module.   An example of this would be in the \"vps\" module have order id",
    "input": {
        "sid": {
            "name": "sid",
            "type": "string",
            "description": "the *Session ID* you get from the [login](#login) call",
            "example": "32ed3ca9119d42eaa4673c8e5c97600e"
        },
        "id": {
            "name": "id",
            "type": "int",
            "description": "",
            "example": 10000
        }
    },
    "output": [
        {
            "name": "vps_id",
            "type": "int",
            "description": "VPS Service ID"
        },
        {
            "name": "vps_server",
            "type": "int",
            "description": "VPS Host Server ID"
        },
        {
            "name": "vps_vzid",
            "type": "string",
            "description": "VPS Host-Side Server ID"
        },
        {
            "name": "vps_type",
            "type": "int",
            "description": "Package ID"
        },
        {
            "name": "vps_cost",
            "type": "float",
            "description": "Service Cost"
        },
        {
            "name": "vps_custid",
            "type": "int",
            "description": "Customer ID"
        },
        {
            "name": "vps_ip",
            "type": "string",
            "description": "IP Address of VPS"
        },
        {
            "name": "vps_status",
            "type": "string",
            "description": "Billing Status"
        },
        {
            "name": "vps_invoice",
            "type": "int",
            "description": "Invoice ID"
        },
        {
            "name": "vps_coupon",
            "type": "int",
            "description": "Coupon ID"
        },
        {
            "name": "vps_extra",
            "type": "string",
            "description": "Additional VPS information"
        },
        {
            "name": "vps_hostname",
            "type": "string",
            "description": "Hostname of the VPS"
        },
        {
            "name": "vps_server_status",
            "type": "string",
            "description": "Server Status of VPS"
        },
        {
            "name": "vps_comment",
            "type": "string",
            "description": "Comments about the VPS"
        }
    ],
    "example": {
        "vps_id": 10000,
        "vps_server": 5,
        "vps_vzid": "windows10000",
        "vps_type": 32,
        "vps_cost": 22,
        "vps_custid": 11803,
        "vps_ip": "206.72.197.216",
        "vps_status": "canceled",
        "vps_invoice": 1011318,
        "vps_coupon": 3507,
        "vps_extra": "[]",
        "vps_hostname": "windows10000",
        "vps_server_status": "deleted",
        "vps_comment": ""
    }
}