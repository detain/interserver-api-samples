{
    "name": "api_vps_get_service",
    "desc": "This Function Applies to the VPS services.\nGets service info for the given ID in the given Module.   An example of this would be in the \"vps\" module have order id",
    "input": {
        "sid": {
            "name": "sid",
            "type": "string",
            "description": "the *Session ID* you get from the [login](#login) call",
            "example": "1652a97fb0efab378cc48284adcd89d6"
        },
        "id": {
            "name": "id",
            "type": "int",
            "description": "",
            "example": 10000
        }
    },
    "output": [
        {
            "name": "vps_id",
            "type": "int",
            "description": "VPS Service ID"
        },
        {
            "name": "vps_custid",
            "type": "int",
            "description": "Customer ID"
        },
        {
            "name": "vps_server",
            "type": "int",
            "description": "VPS Host Server ID"
        },
        {
            "name": "vps_ip",
            "type": "string",
            "description": "IP Address of VPS"
        },
        {
            "name": "vps_ipv6",
            "type": "string",
            "description": ""
        },
        {
            "name": "vps_vzid",
            "type": "string",
            "description": "VPS Host-Side Server ID"
        },
        {
            "name": "vps_currency",
            "type": "string",
            "description": ""
        },
        {
            "name": "vps_type",
            "type": "int",
            "description": "Package ID"
        },
        {
            "name": "vps_order_date",
            "type": "string",
            "description": "Date the Order was Placed"
        },
        {
            "name": "vps_status",
            "type": "string",
            "description": "Billing Status"
        },
        {
            "name": "vps_invoice",
            "type": "int",
            "description": "Invoice ID"
        },
        {
            "name": "vps_coupon",
            "type": "int",
            "description": "Coupon ID"
        },
        {
            "name": "vps_extra",
            "type": "string",
            "description": "Additional VPS information"
        },
        {
            "name": "vps_hostname",
            "type": "string",
            "description": "Hostname of the VPS"
        },
        {
            "name": "vps_server_status",
            "type": "string",
            "description": "Server Status of VPS"
        },
        {
            "name": "vps_comment",
            "type": "string",
            "description": "Comments about the VPS"
        },
        {
            "name": "vps_slices",
            "type": "int",
            "description": "VPS # of Slices"
        },
        {
            "name": "vps_vnc",
            "type": "string",
            "description": "VNC Address for VPS"
        },
        {
            "name": "vps_vnc_port",
            "type": "int",
            "description": ""
        },
        {
            "name": "vps_rootpass",
            "type": "string",
            "description": "Encrypted Root Password for VPS"
        },
        {
            "name": "vps_mac",
            "type": "string",
            "description": "MAC Address of VPS"
        },
        {
            "name": "vps_os",
            "type": "string",
            "description": "OS or Template of VPS"
        },
        {
            "name": "vps_version",
            "type": "string",
            "description": "Version of VPS or possibly OS Type"
        },
        {
            "name": "vps_location",
            "type": "int",
            "description": "Location of the VPS"
        },
        {
            "name": "vps_platform",
            "type": "string",
            "description": "Platform of the VPS"
        },
        {
            "name": "vps_diskused",
            "type": "int",
            "description": "Used Disk Space"
        },
        {
            "name": "vps_diskmax",
            "type": "int",
            "description": "Total Disk Space"
        }
    ],
    "example": {
        "vps_id": 10000,
        "vps_custid": 11803,
        "vps_server": 1537,
        "vps_ip": "",
        "vps_ipv6": null,
        "vps_vzid": "windows10000",
        "vps_currency": "USD",
        "vps_type": 32,
        "vps_order_date": "2012-11-06 22:22:35",
