{
    "name": "api_backups_get_service",
    "desc": "This Function Applies to the Backup Services services.\nGets service info for the given ID in the given Module.   An example of this would be in the \"vps\" module have order id",
    "input": {
        "sid": {
            "name": "sid",
            "type": "string",
            "description": "the *Session ID* you get from the [login](#login) call",
            "example": "5f20dad4a74142bf03686e70b8027b5c"
        },
        "id": {
            "name": "id",
            "type": "int",
            "description": "",
            "example": 10000
        }
    },
    "output": [
        {
            "name": "backup_id",
            "type": "int",
            "description": "ID of the Backup Host Server"
        },
        {
            "name": "backup_server",
            "type": "int",
            "description": "ID of Server the Backup Resides on"
        },
        {
            "name": "backup_username",
            "type": "string",
            "description": "Username on the Backup Server associated with this Order"
        },
        {
            "name": "backup_type",
            "type": "int",
            "description": "Which type of Backup Server"
        },
        {
            "name": "backup_currency",
            "type": "string",
            "description": ""
        },
        {
            "name": "backup_order_date",
            "type": "string",
            "description": "Date the Order was Placed"
        },
        {
            "name": "backup_custid",
            "type": "int",
            "description": "Customer ID"
        },
        {
            "name": "backup_quota",
            "type": "int",
            "description": "Disk Quota"
        },
        {
            "name": "backup_ip",
            "type": "string",
            "description": "IP Address of the Backup Server"
        },
        {
            "name": "backup_status",
            "type": "string",
            "description": "Billing Status"
        },
        {
            "name": "backup_invoice",
            "type": "int",
            "description": "Invoice ID"
        },
        {
            "name": "backup_coupon",
            "type": "int",
            "description": "Coupon ID"
        },
        {
            "name": "backup_extra",
            "type": "string",
            "description": "Additional Backup Information"
        },
        {
            "name": "backup_server_status",
            "type": "string",
            "description": "Server-Side status of this website"
        },
        {
            "name": "backup_comment",
            "type": "string",
            "description": ""
        }
    ],
    "example": {
        "backup_id": 10000,
        "backup_server": 25,
        "backup_username": "st10000",
        "backup_type": 10831,
        "backup_currency": "USD",
        "backup_order_date": "2021-04-26 04:54:55",
        "backup_custid": 723209,
        "backup_quota": 0,
        "backup_ip": "68.168.209.18",
        "backup_status": "expired",
        "backup_invoice": 19239357,
        "backup_coupon": 0,
        "backup_extra": "[]",
        "backup_server_status": "deleted",
        "backup_comment": ""
    }
}