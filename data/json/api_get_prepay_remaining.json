{
    "name": "api_get_prepay_remaining",
    "desc": "Get the PrePay amount available for a given module.",
    "input": {
        "sid": {
            "name": "sid",
            "type": "string",
            "description": "the *Session ID* you get from the [login](#login) call",
            "example": "3e9a37341e5f0a3df4e4b2b710ef3d33"
        },
        "module": {
            "name": "module",
            "type": "string",
            "description": "the module you want to check your prepay amounts on",
            "example": "domains"
        }
    },
    "output": [
        {
            "name": "status",
            "type": "string",
            "description": "Location Status"
        },
        {
            "name": "status_text",
            "type": "string",
            "description": ""
        }
    ],
    "example": {
        "status": "ok",
        "status_text": "0"
    }
}