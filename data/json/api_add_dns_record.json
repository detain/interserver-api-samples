{
    "name": "api_add_dns_record",
    "desc": "Adds a single DNS record",
    "input": {
        "sid": {
            "name": "sid",
            "type": "string",
            "description": "the *Session ID* you get from the [login](#login) call",
            "example": "1652a97fb0efab378cc48284adcd89d6"
        },
        "domain_id": {
            "name": "domain_id",
            "type": "int",
            "description": "",
            "example": 1000
        },
        "name": {
            "name": "name",
            "type": "string",
            "description": "Specify record name",
            "example": "www"
        },
        "content": {
            "name": "content",
            "type": "string",
            "description": "",
            "example": "127.0.0.1"
        },
        "type": {
            "name": "type",
            "type": "string",
            "description": "Specify TYPE of record",
            "example": "A"
        },
        "ttl": {
            "name": "ttl",
            "type": "int",
            "description": "Specify TTL",
            "example": 86400
        },
        "prio": {
            "name": "prio",
            "type": "int",
            "description": "",
            "example": ""
        }
    },
    "output": {
        "return": {
            "name": "return",
            "type": "int",
            "description": "$resp Response of Action. Default: Serialize"
        }
    },
    "example": null
}