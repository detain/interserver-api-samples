{
    "name": "api_webhosting_get_service",
    "desc": "This Function Applies to the Webhosting services.\nGets service info for the given ID in the given Module.   An example of this would be in the \"vps\" module have order id",
    "input": {
        "sid": {
            "name": "sid",
            "type": "string",
            "description": "the *Session ID* you get from the [login](#login) call",
            "example": "32ed3ca9119d42eaa4673c8e5c97600e"
        },
        "id": {
            "name": "id",
            "type": "int",
            "description": "",
            "example": 10000
        }
    },
    "output": [
        {
            "name": "website_id",
            "type": "int",
            "description": "Server ID"
        },
        {
            "name": "website_server",
            "type": "int",
            "description": "ID of the host server this website resides on."
        },
        {
            "name": "website_type",
            "type": "int",
            "description": "Server Type"
        },
        {
            "name": "website_cost",
            "type": "float",
            "description": "Service Cost"
        },
        {
            "name": "website_custid",
            "type": "int",
            "description": "Customer ID"
        },
        {
            "name": "website_ip",
            "type": "string",
            "description": "IP Address"
        },
        {
            "name": "website_status",
            "type": "string",
            "description": "Billing Status"
        },
        {
            "name": "website_invoice",
            "type": "int",
            "description": "Invoice ID"
        },
        {
            "name": "website_coupon",
            "type": "int",
            "description": "Coupon ID"
        },
        {
            "name": "website_extra",
            "type": "string",
            "description": "Additional Website Details"
        },
        {
            "name": "website_hostname",
            "type": "string",
            "description": "Hostname for this website"
        },
        {
            "name": "website_comment",
            "type": "string",
            "description": "Website Comments"
        },
        {
            "name": "website_username",
            "type": "string",
            "description": "Username on the host server associated w\/ this account"
        },
        {
            "name": "website_server_status",
            "type": "string",
            "description": "Server-Side status of this website"
        }
    ]
}