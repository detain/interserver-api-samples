        }
    },
    "output": [
        {
            "name": "id",
            "type": "int",
            "description": "ID of the Coupon"
        },
        {
            "name": "name",
            "type": "string",
            "description": "Country Name in all Capitols"
        },
        {
            "name": "size",
            "type": "int",
            "description": ""
        },
        {
            "name": "url",
            "type": "string",
            "description": ""
        }
    ],
    "example": []
}