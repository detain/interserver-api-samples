    "input": {
        "sid": {
            "name": "sid",
            "type": "string",
            "description": "the *Session ID* you get from the [login](#login) call",
            "example": "5f20dad4a74142bf03686e70b8027b5c"
        }
    },
    "output": [
        {
            "name": "id",
            "type": "int",
            "description": "ID of the Coupon"
        },
        {
            "name": "name",
            "type": "string",
            "description": "Country Name in all Capitols"
        },
        {
            "name": "size",
            "type": "int",
            "description": ""
        },
        {
            "name": "url",
            "type": "string",
            "description": ""
        }
    ],
    "example": []
}